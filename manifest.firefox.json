{
  "name": "Mindenit Answers",
  "version": "1.37",
  "manifest_version": 3,
  "description": "__MSG_extensionDesc__",
  "icons": {
    "72": "img/icon-72.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "72": "img/icon-72.png"
    }
  },
  "background": {
    "scripts": [
      "service-worker-loader.js"
    ],
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.dl.nure.ua/*"
      ],
      "js": [
        "assets/index.tsx-loader-d666d3f5.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "ruleset_1",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "web_accessible_resources": [
    {
      "resources": [
        "img/icon-72.png",
        "assets/chunk-63f7b0b5.js",
        "assets/chunk-7c0c5339.js"
      ],
      "matches": [
        "*://*.dl.nure.ua/*"
      ],
      "use_dynamic_url": false
    }
  ],
  "permissions": [
    "sidePanel",
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "host_permissions": [
    "*://*.dl.nure.ua/*"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "mindenit-answers@mindenit.ua",
      "strict_min_version": "48.0"
    }
  },
  "commands": {
    "trigger-answer": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "__MSG_extension_settings_keyboard_shortcut_trigger_answer__"
    }
  }
}
